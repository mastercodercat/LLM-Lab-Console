# Copy this file to `.env` and fill in real values. Do NOT commit real secrets to the repo.

# Database connection string used by Prisma.
# DATABASE_URL="postgresql://user:password@localhost:5432/dbname?schema=public"
DATABASE_URL=

# GROQ/OpenAI-compatible API key used by `app/lib/groq.ts` for generation calls
# (keep this secret). Example: GROQ_API_KEY="sk-..."
GROQ_API_KEY=

# Optional HTTP(S) proxy to route outbound requests (used by groq.ts)
# Example: PROXY_URL="http://127.0.0.1:3128"
PROXY_URL=
